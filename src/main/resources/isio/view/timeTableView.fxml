<?import javafx.scene.layout.VBox?>

<VBox fx:id="root" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="isio.TimeTableController">
   <children>
      <MenuBar>
         <menus>
            <Menu mnemonicParsing="false" text="File">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#openAddLinkPopUp" text="Import ICS link" />
                  <MenuItem mnemonicParsing="false" text="Export " />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="View">
               <items>
                  <MenuItem mnemonicParsing="false" text="Change the " />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Help">
               <items>
                  <MenuItem mnemonicParsing="false" text="About" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
      <VBox fx:id="calendarVBox">
         <children>
            <HBox fx:id="buttonHBox" alignment="CENTER" maxWidth="1.7976931348623157E308">
               <children>
                  <Button fx:id="previousButton" mnemonicParsing="false" text="&lt;" />
                  <Region maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
                  <Label fx:id="contentLabel" alignment="CENTER" text="contentLabel" />
                  <Region maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
                  <Button fx:id="nextButton" alignment="CENTER_RIGHT" mnemonicParsing="false" text="&gt;" />
               </children>
            </HBox>
            <HBox fx:id="calendarHBox" prefHeight="100.0" prefWidth="200.0" />
