<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/21"
   xmlns:fx="http://javafx.com/fxml/1" fx:controller="isio.ImportController">
   <children>
      <Label text="Import ICS calendar through a link" />
      <Text fx:id="errorText" fill="RED" strokeType="OUTSIDE" strokeWidth="0.0"
         textAlignment="CENTER" />
      <HBox spacing="10.0">
         <children>
            <TextField fx:id="importLink" prefWidth="400.0" HBox.hgrow="ALWAYS" />
            <Button fx:id="importLinkButton" onAction="#addLink" text="Import" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>